// 38) Write a program to find factorial of a number.

#include<iostream>
using namespace std;

int fact(int a , int f=1){
    for(int i=a;i>=1;i--){
         f*=i;
    }
    return f;
}

int main()
{
    int x;
    cout<<"Enter the number: ";
    cin>>x;
    
    cout<<"The factorial of "<<x<<" is: "<<fact(x);
    
return 0;
}
