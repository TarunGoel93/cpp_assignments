// 39) Write a program to print fibonacci series.

#include<iostream>
using namespace std;

int fib(int x){
    int first=0,second=1,next;
    for(int i=2;i<=x;i++){
        next=first+second;
        first=second;
        second=next;
        cout<< next<<" ";
    }
};

int main()
{
    int a;
    cout<<"Enter the number: ";
    cin>>a;
    cout<<fib(a);
    
return 0;
}
