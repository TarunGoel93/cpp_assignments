// 51)write a program to find given number gcd using functions

#include<iostream>
using namespace std;

int gcd(int a,int b){
    int g=1;
    for(int i=1;i<=a&&i<=b;i++){
        if(a%i==0 && b%i==0){
            g=i;
        }
    }
    return g;
};

int main()
{
    int x,y;
   cout<<"Enter the first number: ";
   cin>>x;
   cout<<"Enter the second number: ";
   cin>>y;
   cout<<"The hcf of "<<x<<" and "<<y<<" is: "<<gcd(x,y);
   
}
