// 11) Write a program to determine if the number is odd or even using for loop.

#include<iostream>
using namespace std;

int main()
{
    int a;
    cout<<"Enter the number: ";
    cin>>a;
    
    cout<<"The even number between 2 and "<<a<<" are: "<<endl;
    for(int i=2;i<=a;i++){
        if(i%2==0){
            cout<<i<<" ";
        }
    }
    cout<<endl;
    cout<<"The odd number between 1 and "<<a<<" are: "<<endl;
    for(int j=1;j<=a;j++){
        if(j%2!=0){
            cout<<j<<" ";
        }
    }
   
    return 0;
}
