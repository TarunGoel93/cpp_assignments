// 9) Write a program to print fibonacci series using for loop.

#include<iostream>
using namespace std;

int main()
{
   int num , first=0 , second=1 , next;
   cout<<"Enter the number: ";
   cin>>num;
   
   for(int i=2;i<num;i++){
       next=first+second;
       first=second;
       second=next;
       
       cout<<next<<" ";
   }
   
    return 0;
}
