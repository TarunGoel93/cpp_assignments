// 17)Write a program to make the following pattern.

#include<iostream>
using namespace std;

int main()
{
    int x;
    cout<<"Enter the number of rows: ";
    cin>>x;
    
    for(int i=1;i<=x;i++){
        for(int j=1;j<=i;j++){
            cout<<j;
            }
            cout<<endl;
    }
return 0;
}
